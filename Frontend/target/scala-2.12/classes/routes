
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index()

GET     /query1                    controllers.HomeController.Query1Handler()
GET     /query2                    controllers.HomeController.Query2Handler()
GET     /query3                    controllers.HomeController.Query3Handler()
GET     /query4                    controllers.HomeController.Query4Handler()
GET     /query5                    controllers.HomeController.Query5Handler()

GET     /q1                         controllers.HomeController.query1()
GET     /q2                         controllers.HomeController.query2()
GET     /q3                         controllers.HomeController.query3()
GET     /q4                         controllers.HomeController.query4()
GET     /q5                         controllers.HomeController.query5()

